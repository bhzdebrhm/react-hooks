{
  "version": "0.4.3",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/foo.modern.module.js",
  "source": "src/index.ts",
  "unpkg": "./dist/foo.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@bhzdebrhm/react-hooks",
  "repository": "git://github.com/bhzdebrhm/react-hooks.git",
  "author": "bhzdebrhm",
  "size-limit": [
    {
      "path": "dist/react-hooks.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-hooks.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "husky": "^8.0.2",
    "microbundle-crl": "^0.13.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "immer": "^9.0.16",
    "uuid": "^9.0.0"
  }
}
